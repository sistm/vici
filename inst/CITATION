  bibtype  = "Article",
  title    = "Analyzing cellular immunogenicity in vaccine clinical trials: a new statistical method including non-specific responses for accurate estimation of vaccine efficacy",
  author   = c(person("Edouard", "Lhomme"), person(c("Boris", "P"), "Hejblum"), person("Christine", "Lacabaratz"), person("Aur{\\'e}lie", "Wiedemann"), person("Jean-Daniel", "Leliv{\\`e}re"), person("Yves", "Levy"), person("Rodolphe", "Thi{\\'e}baut"), person("Laura", "Richert")),
  journal  = "Journal of Immunological Methods",
  year     = "2020",
  volume   = "477",
  number   = "",
  pages    = "112711",
  url      = "https://doi.org/10.1016/j.jim.2019.112711",
  note   = "R package version 0.7.2",
  textVersion = paste("Lhomme E, Hejblum BP, Lacabaratz C, Wiedemann A, Lelièvre J-D, Levy Y, Thiébaut R, Richert L (2020). Analyzing cellular immunogenicity in vaccine clinical trials: a new statistical method including non-specific responses for accurate estimation of vaccine efficacy. Journal of Immunological Methods, 477:112711. <doi:10.1016/j.jim.2019.112711>.")
)

